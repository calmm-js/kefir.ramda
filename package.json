{
  "name": "kefir.ramda",
  "version": "0.25.0-alpha.2",
  "description": "Ramda functions lifted to work on Kefir properties",
  "module": "dist/kefir.ramda.es.js",
  "main": "dist/kefir.ramda.cjs.js",
  "scripts": {
    "dist": "concurrently \"npm run dist:umd\" \"npm run dist:umd.min\" \"npm run dist:es\" \"npm run dist:cjs\"",
    "dist:cjs": "    NODE_ENV=           rollup -c -n kefir.ramda -i src/kefir.ramda.js -f cjs -o dist/kefir.ramda.cjs.js",
    "dist:es": "     NODE_ENV=           rollup -c -n kefir.ramda -i src/kefir.ramda.js -f es  -o dist/kefir.ramda.es.js",
    "dist:umd": "    NODE_ENV=dev        rollup -c -n kefir.ramda -i src/kefir.ramda.js -f umd -o dist/kefir.ramda.js",
    "dist:umd.min": "NODE_ENV=production rollup -c -n kefir.ramda -i src/kefir.ramda.js -f umd -o dist/kefir.ramda.min.js",
    "lint": "eslint src test",
    "prepare": "npm run lint && npm run dist && npm run test",
    "report-coverage": "nyc report --reporter=lcov > coverage.lcov && codecov",
    "test": "nyc mocha",
    "watch": "concurrently \"npm run watch:dist\" \"npm run watch:test\"",
    "watch:dist": "watch-run -d 50 -i -p 'src/**/*.js' npm run dist",
    "watch:test": "watch-run -d 50    -p 'dist/kefir.ramda.cjs.js,test/*.js' npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/calmm-js/kefir.ramda.git"
  },
  "keywords": [
    "kefir",
    "ramda",
    "reactive",
    "direct-style",
    "observable",
    "property"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/calmm-js/kefir.ramda/issues"
  },
  "homepage": "https://github.com/calmm-js/kefir.ramda#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-plugin-annotate-pure-calls": "^0.2.2",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "babel-register": "^6.26.0",
    "codecov": "^3.0.0",
    "concurrently": "^3.5.1",
    "eslint": "^4.19.1",
    "kefir": "^3.8.3",
    "mocha": "^5.0.5",
    "nyc": "^11.6.0",
    "rollup": "^0.57.1",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^3.0.0",
    "watch-run": "^1.2.5"
  },
  "dependencies": {
    "kefir.combines": "^4.3.0",
    "ramda": "^0.25.0"
  },
  "peerDependencies": {
    "kefir": "^3.7.0"
  }
}
